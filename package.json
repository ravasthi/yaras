{
  "name": "yaras",
  "version": "3.0.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ravasthi/yaras"
  },
  "url": "https://github.com/ravasthi/yaras/issues",
  "engines": {
    "node": ">=8.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "cross-env": "^5.2.0",
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.5.10",
    "react": "^16.1.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reset-css": "^4.0.1",
    "susy": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@babel/template": "^7.0.0",
    "@babel/types": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-react-app": "^6.1.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.2",
    "coveralls": "^3.0.2",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.0",
    "node-sass-chokidar": "^1.1.0",
    "nyc": "^13.1.0",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts": "^2.0.3",
    "react-styleguidist": "^7.0.4",
    "sinon": "^7.1.1",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^1.0.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "src/index.js",
      "src/registerServiceWorker.js",
      "src/lib/testCommon.js",
      "src/lib/book-snippets/*.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "require": [
      "@babel/register"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "lint:scss": "stylelint --config=stylelint.config.js 'src/**/*.scss' --syntax scss",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch:css": "yarn run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "lint:js": "eslint --config=.eslintrc.js .",
    "start:js": "cross-env NODE_PATH=./src react-scripts start",
    "start": "run-p watch:css start:js",
    "build": "yarn run build:css && cross-env NODE_PATH=./src react-scripts build",
    "test:js": "cross-env NODE_ENV=test NODE_PATH=./src nyc mocha --opts .mocharc 'src/**/*.test.js'",
    "test:js:watch": "cross-env NODE_ENV=test NODE_PATH=./src mocha --opts .mocharc --watch 'src/**/*.test.js'",
    "pretest": "yarn run lint:scss && yarn run build:css && yarn run lint:js",
    "test": "yarn run test:js",
    "posttest": "if [[ ! -z $COVERALLS_REPO_TOKEN ]]; then nyc report --reporter=text-lcov | coveralls; fi",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "eject": "react-scripts eject"
  },
  "optionalDependencies": {
    "fsevents": "^2.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
