{
  "name": "yaras",
  "version": "3.0.5",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ravasthi/yaras"
  },
  "url": "https://github.com/ravasthi/yaras/issues",
  "engines": {
    "node": ">=8.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "cross-env": "^6.0.3",
    "jest-extended": "^0.11.2",
    "localforage": "^1.5.0",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.5.10",
    "react": "^16.1.1",
    "react-dom": "^16.2.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reset-css": "^5.0.1",
    "susy": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@babel/template": "^7.0.0",
    "@babel/types": "^7.0.0",
    "acorn": "^7.1.0",
    "acorn-jsx": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-react-app": "^9.0.2",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^6.7.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "fsevents": "^2.1.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "node-sass-chokidar": "^1.1.0",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts": "^3.2.0",
    "react-styleguidist": "^10.2.1",
    "style-loader": "^1.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js",
      "!src/lib/fontAwesome.js",
      "!src/lib/book-snippets/*.js"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "lint:scss": "stylelint --config=stylelint.config.js 'src/**/*.scss' --syntax scss",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch:css": "yarn run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "lint:js": "eslint --config=.eslintrc.js .",
    "start:js": "cross-env NODE_PATH=./src react-scripts start",
    "start": "run-p watch:css start:js",
    "build": "yarn run build:css && cross-env NODE_PATH=./src react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --coverage",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "eject": "react-scripts eject"
  },
  "comments": {
    "pretest": "yarn run lint:scss && yarn run build:css && yarn run lint:js"
  },
  "optionalDependencies": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
