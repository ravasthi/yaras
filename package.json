{
  "name": "yaras",
  "version": "4.0.6",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ravasthi/yaras"
  },
  "type": "module",
  "url": "https://github.com/ravasthi/yaras/issues",
  "engines": {
    "node": ">=16.0"
  },
  "dependencies": {
    "@craco/craco": "^6.4.3",
