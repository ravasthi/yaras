{
  "name": "yaras",
  "version": "4.0.4",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ravasthi/yaras"
  },
  "url": "https://github.com/ravasthi/yaras/issues",
  "engines": {
    "node": ">=8.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "cross-env": "^7.0.2",
    "jest-extended": "^0.11.2",
    "localforage": "^1.5.0",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.1",
    "picomatch": "^2.2.2",
    "prop-types": "^15.5.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reset-css": "^5.0.1",
    "susy": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-react-display-name": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.12.13",
    "@babel/plugin-transform-react-jsx-self": "^7.12.13",
    "@babel/plugin-transform-react-jsx-source": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@babel/template": "^7.12.13",
    "@babel/traverse": "^7.12.13",
    "@babel/types": "^7.12.13",
    "acorn": "^8.0.4",
    "acorn-jsx": "^5.3.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-jest": "^26.6.2",
    "babel-preset-react-app": "^10.0.0",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "husky": "^4.2.5",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "leaked-handles": "^5.2.0",
    "lint-staged": "^10.5.4",
    "node-sass-chokidar": "^1.5.0",
    "prettier": "^2.0.4",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts": "^4.0.0",
    "react-styleguidist": "^11.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  },
  "resolutions": {},
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js",
      "!src/lib/fontAwesome.js",
      "!src/lib/book-snippets/*.js"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --config=stylelint.config.js",
      "prettier --write"
    ]
  },
  "scripts": {
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch:css": "yarn run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "lint:js": "eslint --config=.eslintrc.js .",
    "start:js": "react-scripts start",
    "start": "run-p watch:css start:js",
    "build": "yarn run build:css && react-scripts build",
    "testprep": "yarn run lint:scss && yarn run build:css && yarn run lint:js",
    "test": "react-scripts test",
    "test:ci": "yarn run testprep && react-scripts test --watchAll=false --coverage",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "eject": "react-scripts eject"
  },
  "comments": {
    "pretest": "yarn run testprep"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
