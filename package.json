    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "app": "link:./src",
    "cross-env": "^7.0.3",
    "install-peers": "^1.0.3",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "picomatch": "^2.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "reset-css": "^5.0.1",
    "susy": "^3.0.7",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/helper-builder-react-jsx": "^7.16.7",
    "@babel/helper-builder-react-jsx-experimental": "^7.12.11",
    "@babel/helper-define-map": "^7.16.7",
    "@babel/helper-regex": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-display-name": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/plugin-transform-react-jsx-self": "^7.16.7",
    "@babel/plugin-transform-react-jsx-source": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@babel/runtime": "^7.17.2",
    "@babel/template": "^7.16.7",
    "@babel/traverse": "^7.17.3",
    "@babel/types": "^7.17.0",
    "acorn": "^8.7.0",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-rewire": "^1.2.0",
    "babel-preset-jest": "^27.5.1",
    "babel-preset-react-app": "^10.0.1",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "husky": "^7.0.4",
    "ignore-styles": "^5.0.1",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "leaked-handles": "^5.2.0",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "react-addons-test-utils": "^15.6.2",
    "react-scripts": "^5.0.0",
    "react-styleguidist": "^11.2.0",
    "sass": "^1.49.9",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "resolutions": {
    "postcss": "^8.3.11"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js",
      "!src/lib/fontAwesome.js",
      "!src/lib/book-snippets/*.js"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --config=stylelint.config.js",
      "prettier --write"
    ]
  },
  "scripts": {
    "lint:scss": "stylelint 'src/**/*.scss'",
    "build:css": "sass --load-path ./src --load-path ./node_modules src:src",
    "watch:css": "yarn run build:css && sass --load-path ./src --load-path ./node_modules src:src --watch",
    "lint:js": "eslint --config=.eslintrc.js .",
    "start": "craco start",
    "build": "craco build",
    "testprep": "yarn run lint:scss && yarn run build:css && yarn run lint:js",
    "test": "craco test",
    "test:ci": "yarn run testprep && craco test --watchAll=false --coverage",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "eject": "react-scripts eject"
  },
  "comments": {
    "pretest": "yarn run testprep"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "packageManager": "yarn@3.1.1"
}
